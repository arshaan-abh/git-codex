{
  "name": "git-codex",
  "version": "0.1.0",
  "description": "Git subcommand for task-oriented worktrees and multi-agent parallel workflows.",
  "license": "MIT",
  "packageManager": "pnpm@10.28.2",
  "type": "module",
  "bin": {
    "git-codex": "dist/cli.js",
    "codex-wt": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "node -e \"import('node:fs/promises').then(fs=>fs.rm('dist',{recursive:true,force:true}))\"",
    "dev": "tsx src/cli.ts",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "commander": "^14.0.1",
    "execa": "^9.6.0",
    "fast-glob": "^3.3.3"
  },
  "devDependencies": {
    "@types/node": "^24.6.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
