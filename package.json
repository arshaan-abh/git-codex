{
  "name": "git-codex",
  "version": "0.1.0",
  "description": "Git subcommand for task-oriented worktrees and multi-agent parallel workflows.",
  "keywords": [
    "git",
    "git-subcommand",
    "git-worktree",
    "worktree",
    "codex",
    "cli",
    "typescript",
    "developer-tools",
    "automation"
  ],
  "license": "MIT",
  "type": "module",
  "bin": {
    "codex-wt": "dist/cli.js",
    "git-codex": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "changeset": "changeset",
    "clean": "node -e \"import('node:fs/promises').then(fs=>fs.rm('dist',{recursive:true,force:true}))\"",
    "dev": "tsx src/cli.ts",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\"",
    "pack:dry-run": "pnpm pack --dry-run",
    "release": "pnpm build && changeset publish",
    "release:check": "pnpm lint && pnpm typecheck && pnpm test && pnpm build && pnpm pack --dry-run",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "version-packages": "changeset version"
  },
  "dependencies": {
    "commander": "^14.0.1",
    "execa": "^9.6.0",
    "fast-glob": "^3.3.3"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@types/node": "^24.6.0",
    "eslint": "^9.39.2",
    "prettier": "3.8.1",
    "prettier-plugin-organize-imports": "4.3.0",
    "prettier-plugin-packagejson": "3.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.28.2",
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
